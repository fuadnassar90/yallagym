<?xml version='1.0' encoding='utf-8'?>
<widget id="com.YallaGym.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Yalla Gym</name>
    <description>
        A sample Apache Cordova application that stands for fitness.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        4kSoftTech
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
    <preference name="SplashScreen" value="res/screens/screen-xhdpi-portrait.png" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreenSpinnerColor" value="#FA9D16" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="StatusBarOverlaysWebView" value="true" />
</widget>
